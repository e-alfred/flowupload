		"nextcloud-l10n": "^0.1.1",
		"vue": "^2.6.11"
	},
	"browserslist": [
		"extends @nextcloud/browserslist-config"
	],
	"engines": {
		"node": ">=10.0.0"
	},
	"devDependencies": {
		"@babel/core": "^7.10.2",
		"@babel/plugin-syntax-dynamic-import": "^7.8.3",
		"@babel/preset-env": "^7.10.2",
		"@nextcloud/browserslist-config": "^1.0.0",
		"@vue/test-utils": "^1.0.3",
		"babel-eslint": "^10.1.0",
